{
  "manifest_version": 3,
  "name": "LyricsXHelper",
  "description": "send current playing yotubue music information to local LyricsX server",
  "version": "1.0.0",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Make this page red"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://www.youtube.com/*",
    "http://localhost:15252"
  ]
}
